{
  "name": "kevinpollet.dev",
  "description": "Source code for https://kevinpollet.dev",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "repository": "github:kevinpollet/kevinpollet.dev",
  "bugs": "https://github.com/gatsbyjs/gatsby/issues",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx}' '*.{js,md,json}'",
    "start": "npm run develop",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^2.13.9",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-images": "^3.1.3",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-source-filesystem": "^2.1.2",
    "gatsby-transformer-remark": "^2.6.2",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-github": "^0.16.19"
  },
  "devDependencies": {
    "husky": "^3.0.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.18.2"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
